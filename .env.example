# Netlify-specific environment variables
# Copy this file to .env.local and fill in your values

# Production environment
REACT_APP_SITE_URL=https://your-site-name.netlify.app

# Contact form endpoint (if using Netlify Forms)
REACT_APP_NETLIFY_FORMS=true

# Analytics and tracking (optional)
REACT_APP_GA_TRACKING_ID=your-google-analytics-id

# API endpoints (if you have any)
REACT_APP_API_URL=https://api.your-domain.com

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CONTACT_FORM=true
